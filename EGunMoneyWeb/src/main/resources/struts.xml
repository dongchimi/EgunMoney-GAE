<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
  <constant name="struts.i18n.encoding" value="UTF-8" />
  <constant name="struts.objectFactory" value="spring" />
  <constant name="struts.actionProxyFactory" value="struts" />
  <constant name="struts.mapper.class" value="struts" />
  <constant name="struts.action.extension" value="xhtml" />
  <constant name="struts.ognl.allowStaticMethodAccess" value="true" />

  <package name="egunmoney-default" extends="struts-default" >
	<action name="index" class="com.appspot.egunmoney.controller.IndexController" method="execute" >
		<result>index.jsp</result>
	</action>
  </package>
  
  <package name="index" extends="struts-default" namespace="/">
	<action name="index" class="com.appspot.egunmoney.controller.IndexController" method="execute" >
		<result>index.jsp</result>
	</action>
  </package>

  <include file="struts-login.xml" />
  <include file="struts-accountBook.xml" />
</struts>