<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="login" namespace="/login" extends="egunmoney-default">
		<action name="login" class="com.appspot.egunmoney.controller.EgunUserController" method="login">
			<result name="success">login.jsp</result>
			<result name="loginFail">login.jsp</result>
		</action>
		
		<action name="view" class="com.appspot.egunmoney.controller.EgunUserController" method="view">
			<result>login.jsp</result>
		</action>
	</package>
</struts>